class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer , Integer> dp = new HashMap<>();
        int res[]={0,0};
        for(int i=0; i<nums.length; i++)
        {
            if(dp.containsKey(target - nums[i]))
            {
                res[0]=i;
                res[1]=dp.get(target - nums[i]);
            }
            else
            {
                dp.put(nums[i] , i);
            }
        }
        return res;
    }
}
